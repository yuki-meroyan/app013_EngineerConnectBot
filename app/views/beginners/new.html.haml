= form_with(model: @beginner, local: true) do |f|
  = f.label :title do
    title
    = f.text_field :title
  = f.label :category do
    category
    = f.text_field :category
  = f.label :language do
    language
    = f.text_field :language
  = f.label :detail do
    detail
    = f.text_field :detail
  = f.submit "登録する",type: "submit"